{
    "name": "color_query_bool",
    "constraints": [
        "unique(obj1)",
        "exclude_color(attrs1)"
    ],
    "program": [
        "equal",
        [
            ["color",
                [
                    "scene",
                    "obj1",
                    "attrs1"
                ]
            ],
            "color1"
        ]
    ],
    "templates": [
        "Is the <attrs1> <obj1> <color1>?"
    ],
    "tokens": [
        "obj1",
        "attrs1",
        "color1"
    ]
}
