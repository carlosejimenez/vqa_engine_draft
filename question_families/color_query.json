{
    "name": "color_query",
    "constraints": [
        "unique(obj1)",
        "exclude_color(attrs1)",
        "max_length(attrs1, 2)"
    ],
    "program": [
        "color",
        [
            "scene",
            "obj1",
            "attrs1"
        ]
    ],
    "templates": [
        "What color is the <attrs1> <obj1>?",
        "Is the <attrs1> <obj1> <color1> or <!color1>?"
    ],
    "tokens": [
        "obj1",
        "attrs1",
        "color1",
        "!color1"
    ]
}
